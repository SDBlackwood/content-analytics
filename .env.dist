KAFKA_BOOTSTRAP_SERVERS=localhost:29092  
KAFKA_GROUP_ID=content-analytics-dev
KAFKA_AUTO_OFFSET_RESET=latest

S3_ENDPOINT_URL=http://localhost:9000 
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_BUCKET=content-analytics
S3_REGION=us-east-1
S3_USE_SSL=false

# Kafka settings Consume 1M records from kafka every 10s
TARGET_SIZE=5000000 
BATCH_SIZE=1000000 
BATCH_POLL_INTERVAL_SECONDS=10

STORAGE_BASE_PATH=events-dev
STORAGE_COMPRESSION=snappy  
STORAGE_FILE_PREFIX=dev-events

LOG_LEVEL=DEBUG
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE=logs/content-analytics-dev.log

ENCRYPTION_ENABLED=false
ENCRYPTION_KEY=some-key

