KAFKA_BOOTSTRAP_SERVERS=localhost:29092  
KAFKA_GROUP_ID=content-analytics-dev
KAFKA_AUTO_OFFSET_RESET=earliest

S3_ENDPOINT_URL=http://localhost:9000 
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_BUCKET=content-analytics
S3_REGION=us-east-1
S3_USE_SSL=false

# Kafka settings Consume 100k records from kafka every 10s
BATCH_SIZE=100000 
BATCH_POLL_TIMEOUT_SECONDS=10
BATCH_POLL_INTERVAL_MS=1000
BATCH_MAX_POLL_INTERVAL_MS=60000

STORAGE_BASE_PATH=events-dev
STORAGE_COMPRESSION=snappy  
STORAGE_FILE_PREFIX=dev-events

LOG_LEVEL=DEBUG
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE=logs/content-analytics-dev.log

ENCRYPTION_ENABLED=false
ENCRYPTION_KEY=some-key

APP_DEBUG=true
APP_ENVIRONMENT=development
APP_HOST=0.0.0.0
APP_PORT=8000

AIRFLOW_UID=1000
